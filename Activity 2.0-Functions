#include <stdio.h>
#include <stdlib.h>

int main()
{
    int f,e, s, m, p,total;
    double avg;
    printf("enter grade:");
    scanf("%d\n%d\n%d\n%d\n%d",&f,&e,&s,&m,&p);

    total=f+e+s+m+p;
    printf("Total marks: %d\n",total);

    avg=total/5;
    printf("AVERAGE: %lf",avg);

    return 0;
}
