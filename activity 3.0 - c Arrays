#include <stdio.h>
#include <stdlib.h>

int main()
{

    int a[10],size, i, sum=0;
    printf("enter size of the array:");
    scanf("%d",&size);
    for (i=0; i<size;i++)
    {
        printf("enter element:");
        scanf("%d",&a[i]);
    }
    printf("\nsum of array elements:");
    for (i=0;i<size;i++)
    {
        sum=sum+a[i];

    }
    printf("%d",sum);

    return 0;
}
